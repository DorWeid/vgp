{
  "name": "gesher-app",
  "description": "VGP",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chokidar": "^2.0.4",
    "del": "^3.0.0",
    "react": "^16.4.0",
    "react-awesome-button": "^4.2.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "rimraf": "^2.6.2"
  },
  "build": {
    "appId": "com.example.electron-cra",
